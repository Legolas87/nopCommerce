@inject IWorkContext workContext
{
    "emptyTable":     "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.EmptyTable")).Value)",
    "info":           "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Info")).Value)",
    "infoEmpty":      "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.InfoEmpty")).Value)",
    "infoFiltered":   "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.InfoFiltered")).Value)",
    "thousands":      "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Thousands")).Value)",
    "lengthMenu":     "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.LengthMenu")).Value)",
    "loadingRecords": "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.LoadingRecords")).Value)",
    "processing":     "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.LoaderIcon")).Value)",
    "search":         "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Search")).Value)",
    "zeroRecords":    "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.ZeroRecords")).Value)",
    @*"paginate": {
        "first":"@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Paginate.First")).Value)",
        "last":"@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Paginate.Last")).Value)",
        "next":"@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Paginate.Next")).Value)",
        "previous":"@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Paginate.previous")).Value)"
    },*@
    @*"paginate": {
        "first":"&#171;",
        "last":"&#187;",
        "next":"&#8250;",
        "previous":"&#8249;"
    },*@
    "paginate": {
        @if ((await workContext.GetWorkingLanguageAsync()).Rtl)
        {
            <text>
                "first":"<i class='k-icon k-i-seek-e'></i>",
                "last":"<i class='k-icon k-i-seek-w'></i>",
                "next":"<i class='k-icon k-i-arrow-w'></i>",
                "previous":"<i class='k-icon k-i-arrow-e'></i>"
            </text>
        }
        else
        { 
            <text>
                "first":"<i class='k-icon k-i-seek-w'></i>",
                "last":"<i class='k-icon k-i-seek-e'></i>",
                "next":"<i class='k-icon k-i-arrow-e'></i>",
                "previous":"<i class='k-icon k-i-arrow-w'></i>"
            </text>        
        }
    },
    "aria": {
        "sortAscending":  "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Aria.SortAscending")).Value)",
        "sortDescending": "@JavaScriptEncoder.Default.Encode((await T("Admin.DT.Aria.SortDescending")).Value)"
    }
}