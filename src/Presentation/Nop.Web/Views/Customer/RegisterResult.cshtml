@model RegisterResultModel
@{
    Layout = "_ColumnsOne";

    //title
    NopHtml.AddTitleParts((await T("PageTitle.Register")).Value);
    //page class
    NopHtml.AppendPageCssClassParts("html-registration-result-page");
}
<div class="page registration-result-page">
    <div class="page-title">
        <h1>@await T("Account.Register")</h1>
    </div>
    <div class="page-body">
        <div class="result">
            @Model.Result
        </div>
        <div class="buttons">
            <a href="@Model.ReturnUrl" class="button-1 register-continue-button">@await T("Common.Continue")</a>
        </div>
    </div>
</div>